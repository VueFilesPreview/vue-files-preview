{
  "name": "vue-files-preview",
  "version": "1.0.41",
  "type": "module",
  "private": false,
  "description": "A tool for previewing files such as doc, excel, pdf, image, markdown, txt, audio, and video and so on.",
  "author": "SmallTeddy",
  "maintainers": [
    {
      "url": "https://github.com/wh131462",
      "name": "EternalHeart",
      "email": "hao131462@qq.com"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/VueFilesPreview",
  "repository": {
    "type": "git",
    "url": "https://github.com/VueFilesPreview/vue-files-preview"
  },
  "bugs": {
    "url": "https://github.com/VueFilesPreview/vue-files-preview/issues"
  },
  "keywords": [
    "vue-file-preview",
    "vue3-file-preview",
    "file-preview",
    "vue-preview",
    "vue3-preview",
    "vue3-viewer",
    "vue3-file-viewer",
    "vue",
    "vue3"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf dist",
    "build-version": "tsx scripts/build-version.ts",
    "prebuild": "run-p clean build-version",
    "build": "vite build && vite build --config vite.config.umd.ts",
    "preview": "vite preview",
    "unpub": "npm unpublish",
    "pub": "cd dist && npm publish --access=public --registry=https://registry.npmjs.com",
    "preexample": "npm run build",
    "example": "cd example && pnpm dev",
    "example:install": "cd example && pnpm install",
    "example:build": "cd example && pnpm build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@codemirror/lang-html": "6.4.9",
    "@codemirror/lang-javascript": "6.2.4",
    "@codemirror/lang-json": "6.0.1",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/theme-one-dark": "6.1.2",
    "@vue-office/docx": "1.6.2",
    "@vue-office/excel": "1.7.14",
    "@vueuse/core": "13.2.0",
    "codemirror": "^6.0.1",
    "epubjs": "0.3.93",
    "highlight.js": "11.11.1",
    "markdown-it": "14.1.0",
    "markdown-it-container": "4.0.0",
    "markdown-it-footnote": "4.0.0",
    "vue": "3.5.12",
    "vue-codemirror": "6.1.1",
    "vue-demi": "0.14.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.13.1",
    "@rollup/plugin-typescript": "12.3.0",
    "@types/node": "25.3.0",
    "@vitejs/plugin-vue": "5.1.4",
    "eslint": "9.27.0",
    "npm-run-all": "4.1.5",
    "rimraf": "6.0.1",
    "sass": "1.97.3",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "tsx": "4.19.2",
    "unplugin-auto-import": "19.2.0",
    "vite": "5.4.11",
    "vite-plugin-dts": "4.3.0",
    "vite-tsconfig-paths": "5.0.1"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}